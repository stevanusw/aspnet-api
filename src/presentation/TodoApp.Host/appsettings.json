{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": "Warning",
    "Override": {
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "SqlConnection": "Server=(localdb)\\mssqllocaldb;Database=Todo;Integrated Security=true"
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "issuer": "TodoApp",
    "audience": "https://localhost:7080",
    "expiresMins": 5
  }
}
